# AV Scanner Configuration
# Copy to .env and fill in values

# Required: PocketBase connection
POCKETBASE_URL=http://localhost:8090
POCKETBASE_SUPERUSER_EMAIL=av@zapuskalka.com
POCKETBASE_SUPERUSER_PASSWORD=1234567890

# Optional: ClamAV settings
CLAMSCAN_PATH=clamscan
WORK_DIR=/tmp/zapuskalka-av-scans
MAX_RETRIES=3
LOG_LEVEL=info
