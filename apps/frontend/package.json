{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "check": "run-p check:lsp check:lint check:format",
    "check:lsp": "vue-tsc --noEmit",
    "check:lint": "eslint .",
    "check:format": "prettier --check ./src",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "npx vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "@nuxt/ui": "^4.2.1",
    "@t3-oss/env-core": "^0.13.8",
    "axios": "^1.13.2",
    "backend-api": "workspace:*",
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.6"
  }
}
