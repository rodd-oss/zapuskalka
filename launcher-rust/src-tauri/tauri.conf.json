{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Zapuskalka",
  "version": "0.1.5",
  "identifier": "com.milanrodd.zapuskalka",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "width": 800,
        "height": 600,
        "label": "main",
        "create": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIzNDk4NTFDQzRERUE5MTEKUldRUnFkN0VISVZKczgra3p6b1p5N0ZiQ3Bod0g5QnpYN0h3VDVSWG9PN0lsZXY1OEhqUHlFK2QK",
      "endpoints": [
        "https://github.com/rodd-oss/zapuskalka/releases/latest/download/latest.json"
      ]
    }
  }
}
