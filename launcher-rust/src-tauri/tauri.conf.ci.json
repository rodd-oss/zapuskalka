{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Zapuskalka",
  "version": "0.0.0-ci",
  "identifier": "com.milanrodd.zapuskalka",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "width": 800,
        "height": 600,
        "label": "main",
        "create": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "createUpdaterArtifacts": false,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}

